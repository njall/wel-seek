        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:element name="studies" type="Studies"></xsd:element>
  <xsd:element name="study" type="Study"></xsd:element>

  <xsd:complexType name="Study">
    <xsd:complexContent>
      <xsd:extension base="MinimalType">
        <xsd:sequence>
          <xsd:element ref="dc:creator" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
