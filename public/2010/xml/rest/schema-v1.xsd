  <xsd:element name="experiments" type="Experiments"></xsd:element>
  <xsd:element name="experiment" type="Experiment"></xsd:element>

  <xsd:complexType name="Experiment">
    <xsd:complexContent>
      <xsd:extension base="MinimalType">
        <xsd:sequence>
          <xsd:element ref="dc:creator" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
          <xsd:element name="submitter" type="Submitter"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="organisms" type="OrganismItems"
                       minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="policy" type="xsd:anyType" minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="experiment_class" type="ResourceLink"
                       maxOccurs="1" minOccurs="1"></xsd:element>
          <xsd:element name="experiment_type" type="ResourceLink"
                       maxOccurs="1" minOccurs="1"></xsd:element>
          <xsd:choice>
            <xsd:element name="technology_type" type="ResourceLink" maxOccurs="1" minOccurs="1"></xsd:element>
            <xsd:element name="technology_type" maxOccurs="1" minOccurs="1" nillable="true"></xsd:element>
          </xsd:choice>
          <xsd:element name="experiment_organisms" type="ExperimentOrganisms"
                       minOccurs="1" maxOccurs="1"></xsd:element>
          <xsd:element name="data_relationships" type="DataRelationships"
                       minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="associated" type="AssociatedItems"
                       minOccurs="1" maxOccurs="1"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="DataRelationships">
    <xsd:sequence>
      <xsd:element name="data_relationship" type="DataRelationship"
                   maxOccurs="unbounded" minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="DataRelationship">
    <xsd:sequence>
      <xsd:element name="data_file" type="ResourceLink"
                   maxOccurs="1" minOccurs="1">
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="type" type="xsd:string" use="required"></xsd:attribute>
  </xsd:complexType>

  <xsd:complexType name="ExperimentOrganisms">
