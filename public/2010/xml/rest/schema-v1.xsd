          <xsd:element name="related" type="xsd:anyType"
                       minOccurs="0" maxOccurs="1">
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="InvestigationItems">
    <xsd:sequence>
      <xsd:element name="investigation" type="InvestigationItem"
