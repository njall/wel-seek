      <xsd:extension base="MinimalType">
        <xsd:sequence>
                    <xsd:element ref="dc:creator" maxOccurs="1" minOccurs="0"></xsd:element>
          <xsd:element name="uuid" type="xsd:string" minOccurs="1"
                       maxOccurs="1"></xsd:element>
                    <xsd:element name="submitter" type="Submitter"
						minOccurs="1" maxOccurs="1"></xsd:element>
                    <xsd:element name="policy" type="xsd:anyType" minOccurs="0" maxOccurs="1"></xsd:element>
          <xsd:element name="associated" type="AssociatedItems"
                       minOccurs="1" maxOccurs="1"></xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

