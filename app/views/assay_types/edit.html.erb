<h1>Edit experiment type</h1>

<div class="show_basic">
  <div style="float:left">
    <ul><%= ontology_editor_display ExperimentType, false, false, @experiment_type.id %></ul>
  </div>      
  <div style="float:left">
    <% form_for @experiment_type do |f| %>
      <%= render :partial => "form", :locals => { :f => f, :action=>:edit } -%>
    <% end -%>
  </div>
</div>